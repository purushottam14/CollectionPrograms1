import java.util.ArrayList;

public class ArrayListDemo {

	public static void main(String[] args) {
		ArrayList al= new ArrayList();
		
		al.add(100);
		al.add("Welcome");
		al.add(15.5);
		al.add('A');
		al.add(true);
		
		System.out.println(al);
		System.out.println(al.size());
		al.remove(1);//here 1 is index
	
	}

}
