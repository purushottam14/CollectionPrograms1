import java.util.List;

public class Employee {
	
	int eid;
	String ename;
	String salary;
	String dept;
	
	public Employee(int eid, String ename, String salary, String dept) {
		
		this.eid = eid;
		this.ename = ename;
		this.salary = salary;
		this.dept = dept;
	}

}
class Main {
	
	public static void main(String [] args) {
		List<Employee> l = new List<Employee>();
		
		
	}
}

